<?xml version="1.0"?>
<template_definition>
	<ID>memory</ID> 
	<name>回憶</name> <!--template的簡短命名-->
	<description>一段喚起人回憶的影片</description> <!--template的描述-->
	<composition>test_compo</composition> <!--要輸出的composition-->
	<customizable_object_list>
		<customizable_object>
			<ID>photo1</ID> <!-- 在.aep中的footage ID-->
			<format>image</format> <!--video, image, sound, text-->
			<description>最浪漫的一張相片</description> <!--這個"洞"的描述-->
			<key_frame_time>2.0</key_frame_time><!--key frame是出現在第幾秒的畫面-->
			<key_frame_preview_start>1.0</key_frame_preview_start><!--key frame預覽是在第幾秒開始-->
			<key_frame_preview_end>2.2</key_frame_preview_end><!--key frame預覽是在第幾秒開始-->
		</customizable_object>
		<customizable_object>
			<ID>photo2</ID> <!-- 在.aep中的footage ID-->
			<format>image</format> <!--video, image, sound, text-->
			<description>最好笑的一張相片</description> <!--這個"洞"的描述-->
			<key_frame_time>4.0</key_frame_time><!--出現在第幾秒的畫面-->
			<key_frame_preview_start>3.9</key_frame_preview_start><!--key frame預覽是在第幾秒開始-->
			<key_frame_preview_end>4.5</key_frame_preview_end><!--key frame預覽是在第幾秒開始-->
		</customizable_object>
	</customizable_object_list>
</template_definition>