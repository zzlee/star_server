<?xml version="1.0"?>
<template_definition>
	<ID>photo</ID> 
	<name>相片</name> <!--template的簡短命名-->
	<description>牆上相片</description> <!--template的描述-->
	<composition>test_compo</composition> <!--要輸出的composition-->
	<customizable_object_list>
		<customizable_object>
			<ID>map</ID> <!-- 此可客制化物件在.aep中的footage的name-->
			<format>image</format> <!--video, image, sound, text-->
			<description>一張有特殊意義的相片</description> <!--這個"洞"的描述-->
			<key_frame_time>2.0</key_frame_time><!--出現在第幾秒的畫面-->
			<key_frame_preview_start>1.0</key_frame_preview_start><!--key frame預覽是在第幾秒開始-->
			<key_frame_preview_end>2.2</key_frame_preview_end><!--key frame預覽是在第幾秒開始-->
			<video_with_tracker>video</video_with_tracker><!--追踪此可客制化物件四個角的tracker所在的footage的name-->
			<tracker_ID>Tracker 1</tracker_ID><!--追踪此可客制化物件四個角的tracker的name-->
			<track_point_upper_left>Track Point 1</track_point_upper_left><!--追踪此可客制化物件左上角的track point的name-->
			<track_point_upper_right>Track Point 2</track_point_upper_right><!--追踪此可客制化物件右上角的track point的name-->
			<track_point_lower_left>Track Point 3</track_point_lower_left><!--追踪此可客制化物件左下角的track point的name-->
			<track_point_lower_right>Track Point 4</track_point_lower_right><!--追踪此可客制化物件右下角的track point的name-->
		</customizable_object>
	</customizable_object_list>
</template_definition>