
form#mainform(action='')

    #condition-inner
      label 查詢影片
      input(class='ugcSearchBtn', type='text' ,name='ugcSearchText' ,onfocus='this.value=\'\'')
      input#ugcSearchBtn(type='button', class='ugcSearch', value='搜尋')
      br
      label
      | 篩選條件
      input#ugcSearchNoRatingBtn(type='button', value='尚未審核')
      input#ugcSearchRatingBtn(type='button', value='已經審核')
      input#ugcSearchAllBtn(type='button', value='All')
      | and / or
      label 投件時間
      input(class='ugcSearchDateBtn', type='datetime-local', name='TimeStart', onfocus='this.value=\'\'')
      | 到
      input(class='ugcSearchDateBtn', type='datetime-local', name='TimeEnd', onfocus='this.value=\'\'')
      input#ugcSearchDateBtn(type='button', class='ugcSearchDate', value='送出')
    

    table#product-table(border='0', width='100%', cellpadding='0', cellspacing='0')
        tr
          th.table-header-repeat.line-left.minwidth-1(width='10')
